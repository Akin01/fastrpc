{
  "name": "@akin01/fastrpc",
  "version": "0.0.2",
  "description": "A high performance RPC framework for Deno, supporting TCP transport and MessagePack serialization.",
  "license": "MIT",
  "tasks": {
    "example-server": "deno run -A examples/hello-rpc/server.ts",
    "example-client": "deno run -A examples/hello-rpc/client.ts",
    "test": "deno test -A tests/"
  },
  "imports": {
    "@opentelemetry/api": "npm:@opentelemetry/api@^1.9.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/msgpack": "jsr:@std/msgpack@^1.0.3"
  },
  "exports": {
    ".": "./mod.ts",
    "./decorator": "./rpc/decorator.ts",
    "./rpcHandler": "./rpc/rpcHandler.ts",
    "./tcpTransport": "./rpc/tcpTransport.ts",
    "./middleware": "./rpc/middleware.ts",
    "./types": "./rpc/types.ts",
    "./serializer": "./rpc/serializer.ts",
    "./tlsConfig": "./rpc/tlsConfig.ts",
    "./tracing": "./rpc/tracing.ts",
    "./stream": "./rpc/stream.ts"
  },
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
